import java.lang.Exception;
class Try
{
  public static void main(String args[])
  {
     try 
     {
     int a=10;
     int b;
     for(b=10; b>=0;b--)
     {
       System.out.println(a/b);
      }
      }
  catch(ArithmeticException e)
  {
     System.out.println(e.toString());
   }  
 } 
}

OUTPUT:-
1
1
1
1
1
2
2
3
5
10
java.lang.ArithmeticException: / by zero
