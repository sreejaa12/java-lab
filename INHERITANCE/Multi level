import java.util.*;
class A{
public void methodA()
{
  System.out.println("parent class");
 }
}
class B extends A{
    public void methodB()
    {
       System.out.println("Child class");
     }
   }
     
class Multi_level extends B {
 public void methodC()
 {
    System.out.println("child class 2");
 }
  public static void main(String args[])
     {
        Multi_level ml = new Multi_level();
        ml.methodA();
        ml.methodB();
        ml.methodC();       
      }
   }
         
       
   OUPUT:-
parent class
Child class
child class 2
